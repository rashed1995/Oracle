<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UnitOfMeasureLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.lov.UnitOfMeasureLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select distinct muomv.UNIT_OF_MEASURE --muc.unit_of_measure booking_uom,muc.conversion_rate
  from MTL_UOM_CLASSES_VL      mucv,
       MTL_UOM_CONVERSIONS     muc,
       MTL_UNITS_OF_MEASURE_VL muomv
 where muomv.UOM_CLASS = mucv.UOM_CLASS
   and mucv.UOM_CLASS = muc.uom_class
   --and muomv.BASE_UOM_FLAG = 'Y' 
   and muc.inventory_item_id = 0
   and muomv.UNIT_OF_MEASURE in ('Yard', 'Set', 'Meter', 'Piece')]]>
  </SQLQuery>
  <ViewAttribute
    Name="UnitOfMeasure"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UNIT_OF_MEASURE"
    Expression="UNIT_OF_MEASURE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
        <AUTOSUBMIT
          Value="true"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
